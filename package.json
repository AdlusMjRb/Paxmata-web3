{
  "name": "rfqbiddingplatform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npx hardhat compile",
    "deploy-testnet": "npx hardhat run ./scripts/LifeJacketProjectToken.js --network testnet",
    "deploy": "npx hardhat run ./scripts/LifeJacketProjectToken.js",
    "node": "npx hardhat node",
    "deploy-local": "npx hardhat run ./scripts/LifeJacketProjectToken.js --network localhost",
    "start": "node server.js",
    "start-ai": "AI_SERVER_PORT=3002 node serverAi.js",
    "start-web3": "PORT_WEB3=3003 node serverWeb3.js",
    "start-all": "concurrently \"npm run start\" \"npm run start-web3\" \"npm run start-ai\""
  },
  "dependencies": {
    "@ethersproject/experimental": "^5.7.0",
    "@google-cloud/language": "^6.3.0",
    "@google-cloud/storage": "^7.11.2",
    "@google-cloud/vision": "^4.3.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^4.9.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "ethers": "^5.7.2",
    "express": "^4.19.2",
    "mongodb": "^6.4.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.40.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "concurrently": "^8.2.2",
    "hardhat": "^2.22.6"
  }
}
